<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:id="@+id/row">

    <TextView
        android:text="Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/name" />

    <TextView
        android:text="Hitbox Active"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/hitboxActive" />

    <TextView
        android:text="FAF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/faf" />

    <TextView
        android:text="Damage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/damage" />

    <TextView
        android:text="Angle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/angle" />

    <TextView
        android:text="BKB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bkb" />

    <TextView
        android:text="KBG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/kbg" />

    <TextView
        android:text="Landing Lag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/landingLag" />

    <TextView
        android:text="AutoCancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/autoCancel" />
</TableRow>